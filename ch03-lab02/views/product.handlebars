
<h3><b>Product List</b></h3>

<div style="display: flex">
{{#each product}}
<div style="width: 350px;height: 333px; margin-right:10px;">
  <p style="margin-left: 15px;">{{this.name}}</p>
  <img src="{{this.image}}" alt="{{this.name}}" style="max-width: 100%;max-height: 100%">
  </div>
{{/each}}
</div>